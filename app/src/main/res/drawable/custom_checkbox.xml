<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_checked="true" android:state_pressed="true"
		android:drawable="@drawable/checked_checkbox" />
	<item android:state_checked="false" android:state_pressed="true"
		android:drawable="@drawable/normal_checkbox" />
	<item android:state_checked="false" android:drawable="@drawable/normal_checkbox" />
	<item android:state_checked="true" android:drawable="@drawable/checked_checkbox" />
</selector>
